package djikstra;

import java.util.PriorityQueue;

public class DjikstraPQ {
	PriorityQueue<Node> pq;
	
	public void insert(int index, int distance){
		pq.add(new Node(index, distance));
	}
	
	public Node getMinElement(){
		return pq.remove();
	}
	
	public boolean isEmpty(){
		return pq.isEmpty();
	}
}
